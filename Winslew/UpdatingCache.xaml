<Window x:Class="Winslew.UpdatingCache"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Updating Cache" Height="186" Width="373" Icon="/Winslew;component/Images/refresh.png" WindowStyle="None" WindowStartupLocation="CenterScreen">
    <Window.Background>
        <LinearGradientBrush StartPoint="0,0" EndPoint="0,0.2">
            <LinearGradientBrush.GradientStops>
                <GradientStop Offset="0" Color="White" />
                <GradientStop Offset="1" Color="LightGray" />
            </LinearGradientBrush.GradientStops>
        </LinearGradientBrush>
    </Window.Background>

    <Grid>
        <Label Height="44" Margin="12,12,53,0" Name="label1" VerticalAlignment="Top" FontSize="24">Updating cache...</Label>
        <ProgressBar Height="26" Margin="12,62,12,0" Name="progressBar1" VerticalAlignment="Top" SmallChange="1" />
        <Image Height="32" Margin="12,103,0,0" Name="image1" Stretch="Fill" VerticalAlignment="Top" HorizontalAlignment="Left" Width="32" Source="/Winslew;component/Images/Winslew.ico" />
        <Label Height="28" Margin="50,107,12,0" Name="label2" VerticalAlignment="Top" FontSize="12">Winslew</Label>
    </Grid>
</Window>
